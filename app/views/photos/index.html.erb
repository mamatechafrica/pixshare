<p style="color: green"><%= notice %></p>

<h1>Photos</h1>

<div id="photos">
<% if @photos %>
    <% @photos.each do |photo| %>
      <%= render photo %>
      <p>
        <%= link_to "Show this photo", photo %>
    <% end %>
  <% else %>
    <p>No photos found.</p>
  <% end %>
</div>

<%= link_to "New photo", new_photo_path %>
